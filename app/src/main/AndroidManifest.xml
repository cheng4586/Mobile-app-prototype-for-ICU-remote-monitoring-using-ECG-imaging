<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nouman.sceneview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nouman.sceneview.RegisterActivity"></activity>
        <activity android:name="com.nouman.sceneview.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nouman.sceneview.TestListActivity" />
        <activity android:name="com.nouman.sceneview.PatientViewActivity" />
        <activity
            android:name="com.nouman.sceneview.SceneViewActivity"
            android:theme="@style/MonitoringTheme" />
        <activity
            android:name="com.nouman.sceneview.MainActivity"
            android:label="Menu"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>